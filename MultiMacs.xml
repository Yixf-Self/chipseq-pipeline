<?xml version="1.0" encoding="ISO-8859-1" ?>
<meta:metadata xmlns:meta="http://cancerresearchuk.org/pipeline-meta/1.2"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://cancerresearchuk.org/pipeline-meta/1.2 http://uk-cri-lsol03:8080/solexa/solexa01/schema/workflow/metadata-1.2.xsd">

    <mode>lsf</mode>
    <pipeline>/lustre/mib-cri/carrol09/Work/MyPipe/Process10/src/main/pipelines/MultiMacsCallingPipeline.xml</pipeline>
    <tempDirectory>${WorkingDirectory}/temp</tempDirectory>
    <summaryFile errorsOnly="true">${WorkingDirectory}/errors</summaryFile>
    <taskDirectories>
        <directory>/lustre/mib-cri/carrol09/Work/MyPipe/Process10/src/main/tasks</directory>
    </taskDirectories>

    <executionConfiguration>
        <execution mode="lsf">
            <lsfOutputDirectory>${temp}/JobOutputs</lsfOutputDirectory>
            <queue>bioinformatics</queue>
            <maximumSubmittedJobs>75</maximumSubmittedJobs>
        </execution>
        <execution mode="local">
            <maxCpuResources>1</maxCpuResources>
        </execution>
    </executionConfiguration>

    <variables>
        <runDir>RunDirectory_ToChange</runDir>
        <BamDirectory>BamDirectory_ToChange</BamDirectory>
	<WorkingDirectory>WorkingDirectory_ToChange</WorkingDirectory>
        <genomeFile>GenomeLengths_ToChange</genomeFile>
    </variables>


  <specialisations>
    <specialisation identifier="Indentifier_ToChange" active="true">
      <variables>
	<Test>Test_ToChange</Test>
	<Control>Control_ToChange</Control>
	<GM>GM_ToChange</GM>
	<SS>SS_ToChange</SS>
	<MF>MF_ToChange</MF>
	<GT>GT_ToChange</GT>
	<GS>GS_ToChange</GS>
    </variables>
  </specialisation>


  </specialisations>


