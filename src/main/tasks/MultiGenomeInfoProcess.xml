<?xml version="1.0" encoding="ISO-8859-1" ?>
<task:task xmlns:task="http://cancerresearchuk.org/task/1.2"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://cancerresearchuk.org/task/1.2 http://uk-cri-lsol03:8080/solexa/solexa01/schema/workflow/task-1.2.xsd">

    <name>MultiGenomeInfoProcess</name>
    <description>Splits the WigProcess pipeline to run over all the processed Bam files found.</description>

    <program type="inbuilt"/>

    <inputs>
        <input name="bam" required="true" description="The processed Bam file." />
    </inputs>

    <outputs>
        <output name="bam_infos" plurality="multiple" required="true" safeNaming="true" description="Bam processes by pipeline" />
        </outputs>

</task:task>

